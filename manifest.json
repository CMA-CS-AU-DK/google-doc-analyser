{
	"manifest_version": 2,
	"name": "Google Doc Analyser",
	"version": "0.2",
	"description": "Analyse Google Doc revisions",
	"background": {
		"scripts": [
			"setup.js"
		]
	},
	"content_scripts": [
		{
			"matches": [
				"*://docs.google.com/document/d/*"
			], 
			"js": [
				"injected.js"
			], 
			"run_at": "document_end" 
		}
		
	],
	"browser_action": {
		//"default_icon": "icon.png",
		"default_title": "Google Doc Analyser"
	},
	"permissions": [
		"webRequest",
		"webNavigation",
		"tabs",
		"activeTab",
		"contextMenus",
		"downloads",
		"*://docs.google.com/document/d/*"
	]
}